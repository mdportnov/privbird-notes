msgid "Invalid data."
msgstr "Некорректные данные."

msgid "The requested resource was not found."
msgstr "Запрашиваемый ресурс не найден."

msgid ""
"There was an error."
"Try again or contact support!"
msgstr ""
"Произошла ошибка.\n"
"Попробуйте еще раз или обратитесь в службу поддержки!"

msgid "Request limit reached."
msgstr "Достигнут лимит запросов."

msgid "The feedback was successfully sent."
msgstr "Обращение успешно отправлено."

msgid ""
"The note has been successfully created."
"The note will be destroyed after reading it!"
"Don’t forget to copy the link!"
msgstr ""
"Заметка успешно создана.\n"
"Заметка будет уничтожена после прочтения!\n"
"Не забудьте скопировать ссылку!"

msgid "The note was successfully received."
msgstr "Заметка успешно получена."

msgid "You have entered the wrong password. Try again."
msgstr "Вы ввели неправильный пароль. Попробуйте снова."

msgid "Could not find a note."
msgstr "Не удалось найти заметку."

msgid ""
"This note has a password."
"Enter it to view the information."
msgstr ""
"У этой заметки есть пароль.\n"
"Введите его, чтобы просмотреть информацию."

msgid "PrivBird notification"
msgstr "Уведомление от PrivBird"

msgid "fake"
msgstr "фейковая"

msgid "the next time someone reads it, a fake note will be displayed"
msgstr "при следующем прочтении будет отображена фейковая записка"

msgid "the content was destroyed"
msgstr "содержимое уничтожено"

msgid "The {real} note with ID {id} has just been read, {ending}."
msgstr "{real} записка с ID {id} была только что прочитана, {ending}."
