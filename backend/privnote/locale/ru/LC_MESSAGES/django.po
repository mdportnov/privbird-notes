#: validators.py:183
msgid "This field is required."
msgstr "Это поле обязательно к заполнению."

#: fields.py:317
msgid "This field may not be null."
msgstr "Это поле не может быть нулевым."

#: fields.py:701
msgid "Must be a valid boolean."
msgstr "Должно быть допустимым логическим значением."

#: fields.py:766
msgid "Not a valid string."
msgstr "Недопустимая строка."

#: fields.py:767
msgid "This field may not be blank."
msgstr "Это поле не может быть пустым."

#: fields.py:768 fields.py:1881
msgid "Ensure this field has no more than {max_length} characters."
msgstr "Убедитесь, что в этом поле не более {max_length} символов."

#: fields.py:769
msgid "Ensure this field has at least {min_length} characters."
msgstr "Убедитесь, что это поле содержит не менее {min_length} символов."

#: fields.py:816
msgid "Enter a valid email address."
msgstr "Введите действительный адрес электронной почты."

#: fields.py:827
msgid "This value does not match the required pattern."
msgstr "Это значение не соответствует требуемому шаблону."

#: fields.py:838
msgid "Enter a valid \"slug\" consisting of letters, numbers, underscores or hyphens."
msgstr "Введите допустимый «слаг», состоящий из букв, цифр, знаков подчеркивания или дефисов."

#: fields.py:839
msgid "Enter a valid \"slug\" consisting of Unicode letters, numbers, underscores, or hyphens."
msgstr "Введите допустимый \"slug\", состоящий из букв Unicode, цифр, символов подчеркивания или дефисов."

#: fields.py:854
msgid "Enter a valid URL."
msgstr "Введите действительный URL-адрес."

#: fields.py:867
msgid "Must be a valid UUID."
msgstr "Должен быть допустимым UUID."

#: fields.py:903
msgid "Enter a valid IPv4 or IPv6 address."
msgstr "Введите действительный адрес IPv4 или IPv6."

#: fields.py:931
msgid "A valid integer is required."
msgstr "Требуется допустимое целое число."

#: fields.py:932 fields.py:969 fields.py:1005 fields.py:1366
msgid "Ensure this value is less than or equal to {max_value}."
msgstr "Убедитесь, что это значение меньше или равно {max_value}."

#: fields.py:933 fields.py:970 fields.py:1006 fields.py:1367
msgid "Ensure this value is greater than or equal to {min_value}."
msgstr "Убедитесь, что это значение больше или равно {min_value}."

#: fields.py:934 fields.py:971 fields.py:1010
msgid "String value too large."
msgstr "Слишком большое строковое значение."

#: fields.py:968 fields.py:1004
msgid "A valid number is required."
msgstr "Требуется действительный номер."

#: fields.py:1007
msgid "Ensure that there are no more than {max_digits} digits in total."
msgstr "Убедитесь, что общее число цифр не превышает {max_digits}."

#: fields.py:1008
msgid "Ensure that there are no more than {max_decimal_places} decimal places."
msgstr "Убедитесь, что число десятичных разрядов не превышает {max_decimal_places}."

#: fields.py:1009
msgid "Ensure that there are no more than {max_whole_digits} digits before the decimal point."
msgstr "Убедитесь, что до десятичной точки не более {max_whole_digits} цифр."

#: fields.py:1148
msgid "Datetime has wrong format. Use one of these formats instead: {format}."
msgstr "Datetime имеет неправильный формат. Вместо этого используйте один из следующих форматов: {format}."

#: fields.py:1149
msgid "Expected a datetime but got a date."
msgstr "Ожидал дату и время, но получил дату."

#: fields.py:1150
msgid "Invalid datetime for the timezone \"{timezone}\"."
msgstr "Недопустимая дата и время для часового пояса \"{timezone}\"."

#: fields.py:1151
msgid "Datetime value out of range."
msgstr "Значение даты и времени вне допустимого диапазона."

#: fields.py:1236
msgid "Date has wrong format. Use one of these formats instead: {format}."
msgstr "Дата имеет неправильный формат. Вместо этого используйте один из следующих форматов: {format}."

#: fields.py:1237
msgid "Expected a date but got a datetime."
msgstr "Ожидал свидания, но получил дату и время."

#: fields.py:1303
msgid "Time has wrong format. Use one of these formats instead: {format}."
msgstr "Время имеет неправильный формат. Вместо этого используйте один из следующих форматов: {format}."

#: fields.py:1365
msgid "Duration has wrong format. Use one of these formats instead: {format}."
msgstr "Длительность имеет неправильный формат. Вместо этого используйте один из следующих форматов: {format}."

