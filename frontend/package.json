{
  "name": "privbird",
  "version": "0.0.0",
  "scripts": {
    "serve::dev": "cross-env VITE_FEEDBACK_URL=${VITE_FEEDBACK_URL} vite --mode dev",
    "build::dev": "cross-env VITE_FEEDBACK_URL=${VITE_FEEDBACK_URL} vite build --mode dev",
    "serve::prod": "cross-env vite --mode prod",
    "build::prod": "cross-env vite build --mode prod",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "element-plus": "^2.2.18",
    "pinia": "^2.0.21",
    "vee-validate": "^4.7.0",
    "vue": "^3.2.38",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.5",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/node": "^16.11.56",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.12",
    "cross-env": "^7.0.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "typescript": "~4.7.4",
    "unplugin-element-plus": "^0.4.1",
    "vite": "^3.0.9",
    "vue-tsc": "^0.40.7"
  },
  "browserslist": [
    "defaults"
  ]
}
